{
  "name": "react-mailto.js",
  "version": "1.1.0",
  "description": "React MailTo component for easy creation fully configured mailto links",
  "main": "dist/react-mailto.js",
  "scripts": {
    "prepublish": "npm run build && npm run build-min",
    "build": "./node_modules/babel-cli/bin/babel.js ./src/react-mailto.js -o ./dist/react-mailto.js",
    "build-min": "./node_modules/babel-cli/bin/babel.js ./src/react-mailto.js -o ./dist/react-mailto.min.js --minified",
    "done": "npm run build && npm run build-min",
    "test": "jest --testRegex=\".test.js\" --config=./jest.config.json",
    "test-watch": "jest --testRegex=\".test.js\" --watch --config=./jest.config.json",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devlio-team/react-mailto.js.git"
  },
  "keywords": [
    "react",
    "react-component",
    "email",
    "mailto"
  ],
  "author": {
    "name": "Vasyl Zubach",
    "email": "i@tjrus.com",
    "url": "http://tjrus.com"
  },
  "engines": {
    "node": ">=4"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devlio-team/react-mailto.js/issues"
  },
  "homepage": "https://github.com/devlio-team/react-mailto.js#readme",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0",
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/react": "^3.2.12",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.4.5",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "chai-string": "^1.1.6",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "jest": "^21.2.1",
    "prop-types": "^15.6.0",
    "react": ">=15.6.2",
    "react-dom": ">=15.6.2",
    "react-test-renderer": "^16.0.0",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.14.0"
  }
}
